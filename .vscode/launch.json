{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "awshift init",
      "resolveSourceMapLocations": [
        "**"
      ],
      "env": {
        "LOG_LEVEL": "debug"
      },
      "console": "integratedTerminal",
      "sourceMaps": true,
      "program": "${workspaceFolder}/bin/dev.js",
      "args": [
        "init"
      ],
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "ts-node-esm"
      ],
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "awshift test",
      "resolveSourceMapLocations": [
        "**"
      ],
      "env": {
        "LOG_LEVEL": "debug",
        "DEBUG": "*"
      },
      "console": "integratedTerminal",
      "sourceMaps": true,
      "program": "${workspaceFolder}/bin/dev.js",
      "args": [
        "test"
      ],
      "runtimeExecutable": "yarn",
      "runtimeArgs": [
        "ts-node-esm"
      ],
      "outputCapture": "std"
    }
  ]
}
